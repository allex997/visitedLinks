{
  "manifest_version": 3,
  "name": "Настя_Мега_hr",
  "version": "0.1",
   "permissions": ["storage", "activeTab", "tabs"],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
